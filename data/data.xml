<odoo>
    <data noupdate="1">
        <record id="sequence_face_model_permanent" model="ir.sequence">
            <field name="name">Sequence For Permanent Face Models</field>
            <field name="code">seq.face.model.permanent</field>
            <field name="active">TRUE</field>
            <field name="prefix">FM</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <record id="sequence_face_model_temporary" model="ir.sequence">
            <field name="name">Sequence For Temporary Face Models</field>
            <field name="code">seq.face.model.temporary</field>
            <field name="active">TRUE</field>
            <field name="prefix">TempFM</field>
            <field name="padding">6</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>