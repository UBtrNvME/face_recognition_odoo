<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <!-- Task Stages -->

    <record id="project_type_under_consideration" model="project.type">
        <field name="sequence">10</field>
        <field name="name">На рассмотрении</field>
        <field name="case_default" eval="True"/>
    </record>
    <record id="project_type_approved" model="project.type">
        <field name="sequence">11</field>
        <field name="name">Одобрено</field>
        <field name="case_default" eval="True"/>
    </record>
    <record id="project_type_rejected" model="project.type">
        <field name="sequence">12</field>
        <field name="name">Отменено</field>
        <field name="case_default" eval="True"/>
        <field name="fold" eval="True"/>
    </record>
</odoo>
