<odoo>
    <data>
        <record model="ir.ui.view" id="gpodem.list">
            <field name="name">gpodem list</field>
            <field name="model">gpodem.gpodem</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="value"/>
                    <field name="value2"/>
                </tree>
            </field>
        </record>

        <record model="ir.ui.view" id="gpodem.form">
            <field name="name">gpodem form</field>
            <field name="model">gpodem.gpodem</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="value"/>
                        <field name="value2"/>
                    </group>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="gpodem.action_window">
            <field name="name">Gpodem window</field>
            <field name="res_model">gpodem.gpodem</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="Gpodem" id="gpodem.menu_root" web_icon="gpodem,static/description/icon.png"
                  groups="group_user"/>
        <menuitem name="Menu 1" id="gpodem.menu_1" parent="gpodem.menu_root" groups="group_user"/>
        <menuitem name="List" id="gpodem.menu_1_list" parent="gpodem.menu_1"
                  action="gpodem.action_window" groups="group_user"/>
    </data>
</odoo>