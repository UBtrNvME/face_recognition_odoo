<odoo>
    <data>
        <record model="ir.ui.view" id="gpodem.list">
            <field name="name">gpodem list</field>
            <field name="model">gpodem.gpodem</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="value"/>
                    <field name="value2"/>
                </tree>
            </field>
        </record>

        <record model="ir.actions.act_window" id="gpodem.action_window">
            <field name="name">gpodem window</field>
            <field name="res_model">gpodem.gpodem</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem name="gpodem" id="gpodem.menu_root"/>
        <menuitem name="Menu 1" id="gpodem.menu_1" parent="gpodem.menu_root"/>
        <menuitem name="List" id="gpodem.menu_1_list" parent="gpodem.menu_1"
                  action="gpodem.action_window"/>
    </data>
</odoo>