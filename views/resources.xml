<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <!-- Adds all assets in Odoo -->
        <template id="assets_backend" name="project_website_valis_assets" inherit_id="web.assets_backend">
            <xpath expr="script[@src='/project/static/src/js/project.js']" position="replace">
                <script type="text/javascript" src="/website_valis/static/js/project.js"></script>
            </xpath>
        </template>
    </data>
</odoo>
